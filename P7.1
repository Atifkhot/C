// Days of week from number 1 to 7
#include <stdio.h>

int main()
{
    int week;
    
    printf("Enter any number from 1 to 7 : ");
    scanf("%d",&week);
    
        switch(week)
        {
        case 1:
        printf("\nMONDAY");
        break;
        
        case 2:
        printf("\nTuesday");
        break;
        
        case 3:
        printf("\nWednesday");
        break;
        
        case 4:
        printf("\nThursday");
        break;
        
        case 5:
        printf("\nFriday");
        break;
        
        case 6:
        printf("\nSaturday");
        break;
        
        case 7:
        printf("\nSunday");
        break;
        
        default:
        printf("\nInvalid input");
        }
        return 0;
}

