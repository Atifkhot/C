#include <stdio.h>

void swap(int*prt1,int*prt2)
{
    printf("\n a=%d\t b=%d",*prt1,*prt2);
    int c;
     c=*prt1;
    *prt1=*prt2;
    *prt2=c;
    
    printf("\nAfter swaping a=%d\t b=%d",*prt1,*prt2);
}
void main()
{
    int a=10;
    int b=20;
    
    printf("Before swaping a=%d\t b=%d",a,b);
   swap(&a,&b);
}
